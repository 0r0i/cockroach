# SELECT with no table.

query I
SELECT 1
----
1
1

query II
SELECT 1+1 AS two, 2+2 AS four
----
two four
2   4

# SELECT expression tests.

statement ok
CREATE DATABASE t

statement ok
CREATE TABLE t.abc (a INT PRIMARY KEY, b INT, c INT)

statement ok
INSERT INTO t.abc VALUES (1, 2, 3)

query III
SELECT * FROM t.abc
----
a b c
1 2 3

query IIIIII
SELECT *,* FROM t.abc
----
a b c a b c
1 2 3 1 2 3

query IIII
SELECT a,a,a,a FROM t.abc
----
a a a a
1 1 1 1

query II
SELECT a,c FROM t.abc
----
a c
1 3

query I
SELECT a+b+c AS foo FROM t.abc
----
foo
6

# SELECT of NULL value.

statement ok
CREATE TABLE t.kv (k CHAR PRIMARY KEY, v CHAR)

statement ok
INSERT INTO t.kv (k) VALUES ('a')

query TT
SELECT * FROM t.kv
----
k v
a NULL

query TT
SELECT k,v FROM t.kv
----
k v
a NULL

query T
SELECT k FROM t.kv
----
1 value hashing to 60b725f10c9c85c70d97880dfe8191b3
